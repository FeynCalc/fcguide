\def\D@@wnFill#1#2#3{$\m@th \setbox\z@\hbox{$#2$}%
  #1\xleaders\box\z@\hfill#3$}
\def\Up@@Fill#1#2#3{$\m@th \setbox\z@\hbox{$#2$}%
