\long\def\DoLongFutureLet #1#2#3#4{% 
    \def\@FutureLetDecide{% 
        #1#2\@FutureLetToken
            \def\@FutureLetNext{#3}%
        \else
            \def\@FutureLetNext{#4}%
        \fi
        \@FutureLetNext
    }%     
\futurelet\@FutureLetToken\@FutureLetDecide}
\def\DoFutureLet #1#2#3#4{\DoLongFutureLet{#1}{#2}{#3}{#4}}

\def\EachCharacter #1\EndEachCharackter{%
  \@EachCharacter #1\@EndEachCharacter
}

\def\@EachCharacter{%
  \DoFutureLet{\ifx}{\@EndEachCharacter}%
    {\@EachCharacterDone}%
	{\@PickUpTheCharacter}%
}

% Set up the test for digits
% This work currently only for 
% double struck charackters
% but it is open to future
% extensions

\def\m@keCharacter#1{\if#10{\csname\F@ntPrefix Zero\endcsname}%
                     \else
                       \if#11{\csname\F@ntPrefix One\endcsname}%
                       \else
                         \if#12{\csname\F@ntPrefix Two\endcsname}%
                         \else
                           \if#13{\csname\F@ntPrefix Three\endcsname}%
                           \else
                             \if#14{\csname\F@ntPrefix Four\endcsname}%
                             \else
                               \if#15{\csname\F@ntPrefix Five\endcsname}%
                               \else
                                 \if#16{\csname\F@ntPrefix Six\endcsname}%
                                 \else
                                   \if#17{\csname\F@ntPrefix Seven\endcsname}%
                                   \else
                                     \if#18{\csname\F@ntPrefix Eight\endcsname}%
                                     \else
                                       \if#19{\csname\F@ntPrefix Nine\endcsname}%
                                       \else
                                         \csname\F@ntPrefix#1\endcsname
                                       \fi
                                     \fi
                                   \fi
                                 \fi
                               \fi
                             \fi
                           \fi
                         \fi
                       \fi
                     \fi}

\def\@PickUpTheCharacter#1{%
  \m@keCharacter{#1}
  \@EachCharacter
}

\def\@EachCharacterDone \@EndEachCharacter{}

\DeclareRobustCommand*{\mathbb}[1]{%
