\def\D@wnFill#1#2#3#4{$\m@th \setbox\z@\hbox{$#2$}%
  #1\leaders\copy\z@\hfill#3
    \xleaders\box\z@\hfill#4$}
\def\Up@Fill#1#2#3#4{$\m@th \setbox\z@\hbox{$#2$}%
  #1\xleaders\copy\z@\hfill#3
  \xleaders\box\z@\hfill#4$}

\def\overbrace#1{\mathop{\vbox{\m@th\ialign{##\crcr\noalign{\kern3\p@}%
      \D@wnFill{\braceUp@L}{\braceUp@Rep}{\braceUp@Mid}{\braceUp@R}\crcr%
	  \noalign{\kern3\p@\nointerlineskip}%
      $\hfil\displaystyle{#1}\hfil$\crcr}}}\limits}
\def\underbrace#1{\mathop{\vtop{\m@th\ialign{##\crcr
   $\hfil\displaystyle{#1}\hfil$\crcr
   \noalign{\kern3\p@\nointerlineskip}%
   \Up@Fill{\braceDown@L}{\braceDown@Rep}{\braceDown@Mid}{\braceDown@R}\crcr%
   \noalign{\kern3\p@}}}}\limits}

