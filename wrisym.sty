\def\overparen#1{\mathop{\vbox{\m@th\ialign{##\crcr\noalign{\kern3\p@}%
      \D@@wnFill{\parenUp@L}{\parenUp@Rep}{\parenUp@R}\crcr%
	  \noalign{\kern3\p@\nointerlineskip}%
      $\hfil\displaystyle{#1}\hfil$\crcr}}}\limits}
\def\underparen#1{\mathop{\vtop{\m@th\ialign{##\crcr
   $\hfil\displaystyle{#1}\hfil$\crcr
   \noalign{\kern3\p@\nointerlineskip}%
   \Up@@Fill{\parenDown@L}{\parenDown@Rep}{\parenDown@R}\crcr%
   \noalign{\kern3\p@}}}}\limits}

