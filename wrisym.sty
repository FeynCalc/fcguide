\newcommand{\rightharpoonupfill}{%
  $\m@th \@extend \mkern-6mu
  \cleaders\hbox{$\mkern-2mu \@extend \mkern-2mu$}\hfill
  \mkern-6mu \mathord\rightharpoonup$}

\newcommand{\leftharpoonupfill}{%
  $\m@th \mathord\leftharpoonup \mkern-6mu
  \cleaders\hbox{$\mkern-2mu \@extend \mkern-2mu$}\hfill
  \mkern-6mu \@extend$}

\newcommand{\lrharpoonupfill}{%
  $\m@th \mathord\leftharpoonup \mkern-6mu
  \cleaders\hbox{$\mkern-2mu \@extend \mkern-2mu$}\hfill
  \mkern-6mu \rightharpoonup$}

\newcommand{\overrightharpoon}[1]{%
  \vbox{\ialign{##\crcr
  \rightharpoonupfill\crcr\noalign{\kern-1pt\nointerlineskip}
  $\hfil\displaystyle{#1}\hfil$\crcr}}}

\newcommand{\overleftharpoon}[1]{%
  \vbox{\ialign{##\crcr
  \leftharpoonupfill\crcr\noalign{\kern-1pt\nointerlineskip}
  $\hfil\displaystyle{#1}\hfil$\crcr}}}

\newcommand{\overlrharpoon}[1]{%
  \vbox{\ialign{##\crcr
  \lrharpoonupfill\crcr\noalign{\kern-1pt\nointerlineskip}
  $\hfil\displaystyle{#1}\hfil$\crcr}}}

